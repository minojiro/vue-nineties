{
  "name": "vue-nineties",
  "version": "0.0.0",
  "description": "Nineties-looking component library for Vue3",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-nineties.umd.js",
  "module": "./dist/vue-nineties.es.js",
