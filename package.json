    "@vitebook/theme-default": "^0.23.4",
    "@vitebook/vue": "^0.23.4",
    "@vue/compiler-sfc": "^3.2.26",
    "svelte": "^3.43.1",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vitest": "^0.5.9",
    "vue": "^3.0.0",
